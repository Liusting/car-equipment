<swiper  current="{{currtab}}" class="swiper-box" duration="300" style="height:{{deviceH-56}}px" bindchange="tabChange">
<swiper-item>
    <scroll-view class="hot-box" scroll-y="true" style="height:{{deviceH-56-64}}px">
        <view  wx:for="{{5}}" wx:key=" " >
        	<view class="cu-card article {{isCard?'no-card':''}}">
		<view class="cu-item shadow" wx:for="{{5}}" wx:key>
			<view class="title">
				<view class='height15'></view>
			</view>
			<view class="content" bindtap='spClick'>
				<image mode="aspectFill" style="  height: 250rpx;width: 250rpx;" bindtap='spClick' data-id='{{item.id}}'
					data-shopId='{{item.shopId}}' data-shopName='{{item.shopName}}'
					src="https://ss0.bdstatic.com/70cFvHSh_Q1YnxGkpoWK1HF6hhy/it/u=3454967093,1824291965&fm=26&gp=0.jpg"></image>
				<view class="desc">
